// 15-7.cpp : This file contains the 'main' function. Program execution begins and ends there.

#include <iostream>
#include <string>
using namespace std;
int main()
{
    int x = 0;
    const char * a = "657";
    x = atoi(a);//String (symbol on ascii table) => integer
    int y = 0;
    y = atoi("22");
    cout << x + y;
    return 0;
}
