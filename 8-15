// 8-15.cpp : This file contains the 'main' function. Program execution begins and ends there.

#include <iostream>
using namespace std;
int square(int x);
double square(double x);

int main()
{
    cout << square(5) << endl;
    cout << square(5.6) << endl;

    
}

int square(int x)
{
    return x * x;
}

double square(double x)
{
    return x * x;
}
