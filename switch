#include <iostream>
using namespace std;
int main()
{
    int i, q, r, q2, r2, q3, q4, r5, r3, r4, q5;
    cout << "Enter a 5-digit number: ";
    cin >> i;
    q = i / 10000;
    r = i % 10000;
    q2 = r / 1000;
    r2 = r % 1000;
    q3 = r2 / 100;
    r3 = r % 100;
    q4 = r3 / 10;
    r4 = r % 10;
    q5 = r4 / 1;

    if ((q == q5) && (q2 == q4))
    {
        cout << "The number is a palindrome" << endl;
    } 
    else
    {
        cout << "The number is not a palindrome" << endl;
    }

    return 0;
}
