// nFactorial.cpp : This file contains the 'main' function. Program execution begins and ends there.
#include <iostream>
using namespace std;

int main()
{
    int n = 1;
    int print = 0;
    int x = 0;
    do
    {
        x = 3 * n + 10;
        n = n + 2;
    } 
    while (x <= 1000);
    cout << n - 2;
    return 0;
}
