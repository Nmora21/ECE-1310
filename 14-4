// 14-4.cpp : This file contains the 'main' function. Program execution begins and ends there.

#include <iostream>

void cube(int*);
using namespace std;

int main()
{
    int n;
    cout << "enter an integer...\n";
    cin >> n;
    cout << "\n the cube of " << n << " is ...";
    cube(&n);
    cout << n << '\n';
    return 0;
}

void cube(int* number)
{
    *number = (*number) * (*number) * (*number);
}
