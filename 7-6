// 7-6.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>
using namespace std;

int main()
{
    int m = rand() % 4;
    int n = 1 + rand() % 5;
    int prod = 1;
    int sum = 0;
    for (int i = 1; i <= m; i++)
    {
        for (int j = 1; j <= n; j++)
        {
          prod = prod * j;
        }
         sum = sum + prod;
    }
    cout << sum << endl;
    cout << "m is " << m << endl;
    cout << "n is " << n << endl;
}
